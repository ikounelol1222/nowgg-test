      let step = -~((1.6 * mask * defaultSize) / alphabet.length);
      return (size = defaultSize) => {
        let id = ''
        while (true) {
          let bytes = getRandom(step)
          let j = step
          while (j--) {
            id += alphabet[bytes[j] & mask] || ''
            if (id.length === size) return id
